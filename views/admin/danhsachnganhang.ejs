   <body>
      <div class="page">
         <!-- Main Navbar-->
         <!-- Header -->
      <%-include('common/header')%>
         <div class="page-content d-flex align-items-stretch">
            <!-- Side Navbar -->
            <%-include('common/content-left')%>
            <div class="content-inner">
               <!-- Page Header-->
               <header class="page-header">
                  <div class="container-fluid">
                     <h2 class="no-margin-bottom">Danh Sách Ngân Hàng</h2>
                  </div>
               </header>
               <!-- Thêm Ngân Hàng-->
               <div>
                <div class="container" style="background: #fff;padding:10px 10px">
                <div class="mb-2" style="color: #5be492">Thêm Ngân Hàng</div>
                <hr/>
                    <form action="/admin/them-ngan-hang" method="post">
                    <div class="container-fluid">
                        <div class="row">
                             <div class="col-md-2 col-sm-12 col-sx-4">
                                <span>Tên Ngân Hàng </span>
                             </div>
                             <div class="col-md-3 col-sm-12 col-sx-8">
                                <input type="text" class="form-control" id="usr" style="display:inline; padding:0;border-radius: 10px;" name="name">
                             </div>
                             <div class="col-md-2 col-sm-12 col-sx-4">
                                <span>Tên Chi Tiết Ngân Hàng </span>
                             </div>
                             <div class="col-md-3 col-sm-12 col-sx-8">
                                <input type="text" class="form-control" id="usr" style="display:inline; padding:0;border-radius: 10px;" name="namenganhang">
                             </div>
                            <div class="col-md-2 col-sm-12 col-sx-12">
                                <button type="submit" name="" id="" class="btn btn-primary" btn-lg btn-block">Thêm</button>
                             </div>
                             
                        <div>
                    </div>
                            
                    </form>
                </div>
               <div>
               <!-- Dashboard Counts Section-->
               <section class="dashboard-counts no-padding-bottom" style="padding-top:10px">
                    <table class="table text-center">
                        <thead>
                            <tr>
                                <th>STT</th>
                                <th>Tên Ngân Hàng</th>
                                <th>Tên Chi Tiết</th>
                                <th>Hành Động</th>
                            </tr>
                        </thead>
                        <tbody>
                        <% banks.forEach((bank,index)=>{
                            %>
                                <tr>
                                <td scope="row"><%=(index+1)%></td>
                                <td> <%=bank.Abbreviations%></td>
                                <td><%=bank.NameBank%></td>
                                <td>
                                    <a name="" id="" class="btn btn-danger" href="/admin/xoa-ngan-hang/<%=bank._id%>" role="button">Xóa</a>
                                </td>
                            </tr>
                           <% 
                        })%>
                        
                        </tbody>
                    </table>
               </section>
               <!-- Dashboard Header Section    -->
               
               <!-- Projects Section-->
            
               <!-- Client Section-->
               
               <!-- Feeds Section-->
              
               <!-- Updates Section                                                -->
             
